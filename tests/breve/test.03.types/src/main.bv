import core;

type Integer = int;

main {
	count: Integer = 0;
	repeat i : 10 {
		repeat _ : i {
			core.Breve.Compiler.Message.ALL;
			core.IO.write("*");
			core.Breve.Compiler.Message.ALL;
			++count;
		}
		core.Breve.Compiler.Message.ALL;
		core.IO.writeLine("*");
		core.Breve.Compiler.Message.ALL;
	}
	core.Breve.Compiler.Message.ALL;
	core.IO.writeLine("Count: ", count);
}