import core.all;

func add(a: real, b: real): real	=> (a + b);
func add(a: int, b: int): int		=> (a + b);

main {
	repeat i: 10
		repeat j: 10 {
			result = add((i as int), (j as int));
			core.IO.writeLine(i, " + ", j, " = ", result);
		}
	repeat i: 10
		repeat j: 10 {
			result = add((i as int), j.negate());
			core.IO.writeLine(i, " - ", j, " = ", result);
		}
}
