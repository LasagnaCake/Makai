import core.convert.tostring;

namespace IO {
	namespace Impl {
		func write(content: string): void {
			in print content;
		}

		func writeLine(content: string): void {
			in print content;
			in print "\n";
		}
	}
	
	macro write		=> {($[...]content)} => {core.IO.Impl.write(Convert.toString($[..., content]))		}
	macro writeLine	=> {($[...]content)} => {core.IO.Impl.writeLine(Convert.toString($[..., content]))	}
}