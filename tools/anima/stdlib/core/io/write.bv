// import core.convert.tostring;

namespace IO {
	namespace Impl {
		func write(content: string): void {
			in print content;
		}

		func writeLine(content: string): void {
			in print content;
			in print "\n";
		}
	}
	
	macro write		=> {($content:vargs{, > $:expr})} => {core.IO.Impl.write(core.Convert.toString($content))		};
	macro writeLine	=> {($content:vargs{, > $:expr})} => {core.IO.Impl.writeLine(core.Convert.toString($content))	};
}