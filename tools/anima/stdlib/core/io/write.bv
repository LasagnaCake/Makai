// import core.convert.tostring;

namespace IO {
	namespace Impl {
		func write(content: string): void {
			in print content;
		}

		func writeLine(content: string): void {
			in print content;
			in print "\n";
		}
	}
	
	macro write		=> {($content:*{, $content:expr})}	=> {core.IO.Impl.write(core.Convert.toString($content:{, $content}))		};
	macro writeLine	=> {($content:*{, $contenst:expr})}	=> {core.IO.Impl.writeLine(core.Convert.toString($content:{, $content}))	};
}