// import core.convert.tostring;

namespace IO {
	namespace Impl {
		func write(what: string): void {
			in print what;
		}

		func writeLine(what: string): void {
			in print what;
			in print "\n";
		}
	}
	
	macro write		=> {(*{, ^what:expr})}	=> {core.IO.Impl.write(core.Convert.toString(^*content{,}))		};
	macro writeLine	=> {(*{, ^what:expr})}	=> {core.IO.Impl.writeLine(core.Convert.toString(^*content{,}))	};
}