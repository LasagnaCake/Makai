namespace Convert {
	namespace Impl {
		macro toString: string => {($value)} => {(($value as string) if ($value is int) else in stringify $value)}
	}

	macro toString: string {
		{()}					=> {""																			}
		{($value)}				=> {core.Convert.Impl.toString($value)											}
		{($first, $[...]rest)}	=> {(core.Convert.Impl.toString($first) + core.Convert.toString($[..., rest]))	}
	}
}

macro toString: string => ($[...]values) => {core.Convert.toString($[..., values])}